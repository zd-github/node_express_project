<% include ./header %>

<!-- 栏目 -->
<div class="container">
	<div class="row">
		<% items.forEach(function(val){ %>
			<div class="col-md-6">
				<div class="list-group">
				  <a href="#" class="list-group-item active">
				    <%= val.name %>
				  </a>
				  <!--该栏目下的文章-->
				 <% val.articleList.forEach(function(value){ %>
					<a href="#" class="list-group-item"> <%= value.title %> </a>
				 <% }) %>
				  
				</div>
			</div>
		<% })%>
	</div>
</div>
<!-- 栏目 end -->

<% include ./footer %>


